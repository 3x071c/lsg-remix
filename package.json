	"engines": {
		"node": ">= 16"
	},
	"os": [
		"linux",
		"darwin"
	],
	"scripts": {
		"build": "cross-env NODE_ENV=production npm run build:internal",
		"build:internal": "run-s clean postinstall generate remix:build server:build",
		"check": "tsc --noEmit",
		"clean": "run-s \"clean:*\"",
		"clean:1": "rimraf .remix",
		"clean:2": "rimraf public/build",
		"clean:3": "rimraf db/zod",
		"commit": "run-s stage cz",
		"cspell:individual": "cspell",
		"cspell:report": "npm run cspell:individual -- \"**\" \".**\"",
		"cz": "cz",
		"dev": "npm run development",
		"development": "cross-env NODE_ENV=development run-s build:internal development:internal",
		"development:internal": "run-p prisma:watch remix:dev remix:server server:watch",
		"eslint:fix": "npm run eslint:template -- --fix .",
		"eslint:individual": "npm run eslint:template -- --fix",
		"eslint:report": "npm run eslint:template -- --max-warnings 0 .",
		"eslint:template": "eslint --cache --report-unused-disable-directives --no-error-on-unmatched-pattern --ext .tsx,.ts,.jsx,.js,.mjs,.cjs --ignore-path .gitignore",
		"fix": "npm run format",
		"format": "run-s prettier:fix eslint:fix secretlint:report cspell:report jscpd:report",
		"generate": "npm run prisma:generate",
		"jscpd:individual": "jscpd",
		"jscpd:report": "npm run jscpd:individual -- \".\"",
		"lint": "run-s prettier:report eslint:report secretlint:report cspell:report jscpd:report",
		"new": "run-s clean generate prisma:reset prisma:dev prisma:seed",
		"postinstall": "remix setup express",
		"preinstall": "npx --yes only-allow npm",
		"prepare": "husky install",
		"prettier:fix": "npm run prettier:individual -- .",
		"prettier:individual": "npm run prettier:template -- --write",
		"prettier:report": "npm run prettier:template -- --check .",
		"prettier:template": "prettier --plugin-search-dir=. --ignore-unknown --ignore-path .gitignore",
		"prisma:dev": "prisma migrate dev --skip-generate --skip-seed",
		"prisma:generate": "prisma generate",
		"prisma:migrate": "prisma migrate dev --skip-generate --skip-seed --name",
		"prisma:prod": "prisma migrate deploy",
		"prisma:push": "prisma db push --skip-generate",
		"prisma:reset": "prisma migrate reset --skip-generate --skip-seed --force",
		"prisma:seed": "prisma db seed",
		"prisma:studio": "prisma studio",
		"prisma:watch": "npm run prisma:generate -- --watch",
		"prod": "npm run production",
		"production": "cross-env NODE_ENV=production run-s clean build start",
		"release": "npm run prisma:prod",
		"remix:build": "remix build",
		"remix:dev": "remix watch",
		"remix:server": "nodemon --inspect --require dotenv/config .remix/server/server.js --watch .remix/server",
		"remix:start": "node .remix/server/server.js",
		"report": "npm run lint",
		"secretlint:individual": "secretlint --secretlintignore .gitignore",
		"secretlint:report": "npm run secretlint:individual -- \".\"",
		"server:build": "esbuild --platform=node --format=cjs app/server.ts --outdir=.remix/server",
		"server:watch": "npm run server:build -- --watch",
		"stage": "git add -A",
		"start": "cross-env NODE_ENV=production npm run remix:start",
		"up": "sudo docker-compose up -d"
	},
